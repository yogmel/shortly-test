{
  "extends": ["next", "next/core-web-vitals", "next/no-html-link-for-pages", "next/no-img-element", "next/no-sync-scripts", "eslint-plugin-react", "eslint-plugin-react-hooks", "prettier"],
  "rules": {
    "indent": [
        "warn",
        2
    ],
    "linebreak-style": [
        "warn",
        "unix"
    ],
    "quotes": [
        "warn",
        "double"
    ],
    "semi": [
        "warn",
        "always"
    ]
  }
}
