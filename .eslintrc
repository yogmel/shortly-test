{
  "extends": ["next", "next/core-web-vitals", "prettier", "plugin:import/recommended", "plugin:import/typescript"],
  "rules": {
    "indent": [
        "warn",
        2
    ],
    "linebreak-style": [
        "warn",
        "unix"
    ],
    "quotes": [
        "warn",
        "double"
    ],
    "semi": [
        "warn",
        "always"
    ]
  }
}
